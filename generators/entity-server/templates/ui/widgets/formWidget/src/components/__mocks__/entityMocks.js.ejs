<%# { "useBluePrint": true, "renameTo": "`/${generator.entityInstance}/formWidget/src/components/__mocks__/${generator.entityInstance}Mocks.js`" } -%>
<%_ include ../../../../utils/jhipster _%>
const <%= entityInstance %>Mock = {
  <%_
    fields.forEach(field => {
      const mockedValue = mockData[0][field.fieldName];
  _%>
  <%= field.fieldName %>: <%- typeof mockedValue === 'string' ? `'${mockedValue}'` : mockedValue %>,
  <%_ }) _%>
};

export default <%= entityInstance %>Mock;
