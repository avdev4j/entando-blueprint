{
  "name": "generator-jhipster-entando",
  "version": "6.2.0",
  "description": "Entando blueprint for generation JHipster services that function as an Entando plugin.",
  "keywords": [
    "yeoman-generator",
    "jhipster-blueprint",
    "jhipster-6"
  ],
  "homepage": "https://entando.com",
  "author": {
    "name": "Entando Inc.",
    "email": "info@entando.com",
    "url": "https://github.com/entando/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entando/entando-blueprint"
  },
  "dependencies": {
    "casual": "^1.6.2",
    "chalk": "^4.1.0",
    "generator-jhipster": "6.10.0",
    "jhipster-core": "^7.3.4",
    "lodash": "^4.17.15",
    "mkdirp": "^1.0.4",
    "shelljs": "^0.8.4",
    "yeoman-environment": "^2.10.3",
    "yeoman-generator": "^4.11.0"
  },
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^9.0.1",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "yaml": "^1.10.0",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "scripts": {
    "dev:setup": "npm install -g generator-jhipster@^6.10.0 yo@^3.1.1",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "prettier --write '**/*.js'",
    "pretest": "npm run lint",
    "test": "npm run test:unit -- test/*.spec.js",
    "test:unit": "mocha --timeout 20000 --slow 0 --reporter spec",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/entando/entando-blueprint/issues"
  }
}
